#include <stdio.h>
#include <string.h>
#include <conio.h>

int main(){
    char str[100];
  
    int cont = 0, i;
    
    printf("Digite frase: ");
    gets(str);
    
    for(i=0; i<(unsigned)strlen(str); i++){
        if(str[i]=='a'){
            cont++;
        }
    }
    printf("Tem %d letras (a)", cont);
}
