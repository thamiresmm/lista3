#include "stdio.h"

int main(){
  int i, j, n, x;
  
  printf("Tamanho vetor: ");
  scanf("%d", &n);

  int v[n];

  for(i=0; i<n; i++){
    printf("Informe valor %d: ", i+1);
    scanf("%d", &x);
    v[i] = x;
  }

  for(i=0; i<5; i++){
    for(j=1; j<=v[i]; j++){      
      if(j<v[i]){
        printf("*");
      }else{
        printf("*\n");
      }
    }
  }
}
